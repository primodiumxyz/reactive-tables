{
  "name": "@primodiumxyz/reactive-tables",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/primodiumxyz/reactive-tables"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./utils": "./dist/utils.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:js",
    "build:js": "tsup",
    "clean": "pnpm run clean:js",
    "clean:js": "rimraf dist",
    "contracts:build": "cd src/__tests__/contracts && pnpm run build",
    "dev": "concurrently -n node,contracts,indexer -c yellow,cyan,red \"cd src/__tests__/contracts && pnpm run devnode\" \"cd src/__tests__/contracts && pnpm run deploy:local\" \"pnpm dev:indexer\"",
    "dev:indexer": "pnpm clean-local-docker-indexer && pnpm local-docker-indexer",
    "test": "pnpm build && vitest",
    "test:verbose": "pnpm build && DEBUG=primodium:* vitest > testLogs.txt"
  },
  "dependencies": {
    "@latticexyz/common": "2.0.6",
    "@latticexyz/protocol-parser": "2.0.6",
    "@latticexyz/schema-type": "2.0.6",
    "@latticexyz/store": "2.0.6",
    "@primodiumxyz/sync-stack": "^0.6.5",
    "react": "^18.2.0",
    "tinybase": "^4.8.0",
    "viem": "2.9.20"
  },
  "devDependencies": {
    "@latticexyz/recs": "^2.0.6",
    "@latticexyz/store-sync": "2.0.6",
    "@latticexyz/world": "2.0.6",
    "@primodiumxyz/pg-indexer-reader": "^0.6.0",
    "@testing-library/react": "^15.0.2",
    "@types/debug": "^4.1.12",
    "@types/node": "^18.19.30",
    "@types/react": "^18.2.78",
    "debug": "^4.3.4",
    "happy-dom": "^14.7.1",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "vite": "^5.2.9",
    "vitest": "^1.5.2"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
